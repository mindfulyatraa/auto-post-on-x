# Example environment variables for local development
# Copy this to .env.local and fill in your actual values

# Backend URL (for production, set this to your Railway URL)
VITE_BACKEND_URL=http://localhost:3001

# X (Twitter) API Credentials (OAuth 1.0a)
VITE_TWITTER_API_KEY=your_api_key_here
VITE_TWITTER_API_SECRET=your_api_secret_here
VITE_TWITTER_ACCESS_TOKEN=your_access_token_here
VITE_TWITTER_ACCESS_SECRET=your_access_secret_here

# X (Twitter) OAuth 2.0 Credentials (Optional)
VITE_TWITTER_CLIENT_ID=your_client_id_here
VITE_TWITTER_CLIENT_SECRET=your_client_secret_here

# Gemini API Key
VITE_GEMINI_API_KEY=your_gemini_api_key_here
